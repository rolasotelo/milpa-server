{
  "name": "la-milpa-server",
  "version": "1.0.1",
  "description": "Server for 'Milpa' online card game",
  "main": "src/index.ts",
  "repository": "git@github.com:rolasotelo/la-milpa-server.git",
  "author": "Rolando Sotelo <rola@hey.com>",
  "keywords": [
    "express",
    "socket.io"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint --cache --ext .js,.ts,.tsx .",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
